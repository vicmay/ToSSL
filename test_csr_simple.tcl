load ./libtossl.so; set keypair [tossl::key::generate -type rsa -bits 2048]; set privkey [dict get $keypair private]; set subject [dict create CN test.example.com]; set csr [tossl::csr::create -key $privkey -subject $subject]; puts "Original CSR:"; puts [tossl::csr::parse $csr]; puts "CSR valid: [tossl::csr::validate $csr]"; puts "CSR fingerprint: [tossl::csr::fingerprint $csr sha256]"
