load ./libtossl.so; set keypair [tossl::key::generate rsa 2048]; set privkey [dict get $keypair private]; set pubkey [dict get $keypair public]; set csr [tossl::csr::create -subject "CN=test.example.com" -pubkey $pubkey -privkey $privkey -san {test.example.com www.test.example.com} -keyusage {digitalSignature keyEncipherment}]; puts "CSR created successfully"; puts [tossl::csr::parse $csr]; puts "CSR valid: [tossl::csr::validate $csr]"; puts "CSR fingerprint: [tossl::csr::fingerprint $csr]"
